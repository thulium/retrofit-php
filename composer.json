{
    "name": "thulium/retrofit-php",
    "description": "Retrofit for PHP - A type-safe PHP REST client",
    "minimum-stability": "stable",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "illuminate/collections": "^10.14.0",
        "letsdrink/ouzo-goodies": "dev-master",
        "nikic/php-parser": "^4.15.5",
        "nyholm/psr7": "^1.8.0",
        "psr/http-message": "^2.0",
        "rawr/t-regx": "^0.41.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10.21",
        "phpunit/phpunit": "^10.2.3"
    },
    "autoload": {
        "psr-4": {
            "Retrofit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Retrofit\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to make requests"
    }
}
